# criar uma lista em forma de dicionario
estoque={"lapis":1,"borracha":2,"caneta":3}
print(estoque)
item1=list(estoque.items())[0]
item2=list(estoque.items())[1]
print(item1,item2)
estoque.update(caderno=5)
print(estoque)
del estoque["caderno"]
print(estoque)
if 'lapis' in estoque:
    print("tem lapis no estoque")
else:
    print("nao tem lapis no estoque")
for i in estoque.keys():
    estoque.update({i:estoque[i]+10})
print(estoque)

